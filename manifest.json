{
  "manifest_version": 3,
  "name": "Angell Fractal Security",
  "version": "0.1.0",
  "description": "Fractal-based network traffic classification using Julia set dynamics. By Nicholas Reid Angell.",
  "author": "Nicholas Reid Angell",
  "permissions": [
    "webRequest",
    "storage",
    "offscreen",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'none';"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}